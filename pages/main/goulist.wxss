.content {
  min-height: 100vh;
  padding: 0 30rpx 60rpx 30rpx;
  box-sizing: border-box;
}
.content .pagetop {
  width: 690rpx;
  padding: 14rpx 0;
  box-sizing: border-box;
  position: relative;
}
.content .listitem {
  padding: 40rpx 0;
  border-bottom: 1rpx solid #f0f0f0;
}
.cardbtn {
  width: 100%;
}
.cardbtn .btntext {
  font-size: 24rpx;
  color: #ff4925;
}
.nodatatips {
  font-size: 28rpx;
  color: #999;
  margin-top: 70rpx;
  line-height: 1.2;
  text-align: center;
}
