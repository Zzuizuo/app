    .content {
        min-height: 100vh;
        box-sizing: border-box;
    }

    .pagetop {
        height: 324rpx;
        padding: 14rpx 30rpx;
        box-sizing: border-box;
        position: relative;
        z-index: 1;
    }

    .pagetoptext {
        text-align: center;
        color: #242627;
        line-height: 1;
        font-size: 32rpx;
        margin-top: 110rpx;
    }

    .pagetopbg {
        position: absolute;
        top: 0;
        left: 0;
        width: 100vw;
        height: 324rpx;
        z-index: -1;
    }

    .title {
        color: #242627;
        line-height: 1;
        font-size: 32rpx;
        margin: 50rpx 0 20rpx 0;
        padding: 0 30rpx;
        font-weight: 500;
        box-sizing: border-box;
    }

    .listbox {
        background: #fafafa;
    }

    .listitem {
        width: 100vw;
        min-height: 240rpx;
        padding: 40rpx 30rpx;
        box-sizing: border-box;
        margin-bottom: 30rpx;
        color: #242627;
        position: relative;
        background: #fff;
    }

    .listitem:last-child {
        margin-bottom: 0;
    }

    .joinbtn {
        width: 140rpx;
        height: 60rpx;
        border-radius: 30rpx;
        color: #fff;
        font-size: 24rpx;
        line-height: 60rpx;
        text-align: center;
        background: #FF4925;
        font-weight: 500;
    }

    .joinsuccessbox {
        display: flex;
        flex-direction: column;
        align-items: center;
        line-height: 1;
    }

    .maskbtn {
        width: 420rpx;
        height: 80rpx;
        line-height: 80rpx;
        text-align: center;
        font-size: 32rpx;
        color: #242627;
        background: #FFDB09;
        border-radius: 40rpx;
        margin: 0 auto;
    }

    .address {
        height: 30rpx;
        line-height: 30rpx;
        padding: 4rpx 12rpx;
        font-size: 22rpx;
        position: absolute;
        right: 0;
        bottom: 0;
        background: #FFDB09;
        border-top-left-radius: 8rpx;
    }

    .searchBox {}

    .search {
        width: 690rpx;
        border-radius: 40rpx;
        background: #FFE54C;
        border: 0
    }

    .search-bg {
        border-radius: 40rpx;
        height: 80rpx;
        display: flex;
        align-items: center;
        text-align: left;
    }

    .search_deletebox {
        width: 60rpx;
        height: 80rpx;
        display: flex;
        align-items: center;
        justify-content: center;
    }

    .search_deletebox image {
        width: 30rpx;
        height: 30rpx;
    }

    .search-result {
        padding: 20rpx;
        margin: 30rpx;
        border-radius: 8rpx;
    }

    .city {
        padding-left: 30rpx;
        padding-right: 10rpx;
        border-right: 1rpx solid #FFDB09;
        display: flex;
        align-items: center;
        font-weight: 600;
    }

    .defaultbox {
        padding-top: 60rpx;
        display: flex;
        flex-direction: column;
        align-items: center;
        line-height: 1;
    }

    .defaultbox image {
        width: 420rpx;
        height: 240rpx;
    }

    .addbtn {
        width: 420rpx;
        height: 80rpx;
        line-height: 80rpx;
        border-radius: 40rpx;
        text-align: center;
        background: #FFDB09;
        margin-top: 80rpx;
        font-size: 32rpx;
        color: #242627;
        font-weight: 500;
    }

    .blankbox {
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        padding-top: 100rpx;
    }

    .blankbox image {
        width: 420rpx;
        height: 244rpx;
    }

    .newsbox {
        width: 288rpx;
        height: 60rpx;
        background: #FF4925;
        position: fixed;
        right: 30rpx;
        bottom: 12rpx;
        border-radius: 10rpx;
    }

    .newsbox::after {
        content: '';
        width: 0;
        height: 0;
        border: 10rpx solid transparent;
        border-top-color: #FF4925;
        position: absolute;
        right: 52rpx;
        bottom: -20rpx;
    }

    .news {
        width: 100%;
        height: 60rpx;
        color: #fff;
        font-size: 24rpx;
        line-height: 60rpx;
        padding: 0 30rpx;
        box-sizing: border-box;
        display: flex;
        align-items: center;
        justify-content: space-between;
    }

    .newsbox2 {
        width: 288rpx;
        height: 60rpx;
        background: #333;
        position: fixed;
        right: 30rpx;
        bottom: 12rpx;
        border-radius: 10rpx;
    }

    .newsbox2::after {
        content: '';
        width: 0;
        height: 0;
        border: 10rpx solid transparent;
        border-top-color: #333;
        position: absolute;
        right: 52rpx;
        bottom: -20rpx;
    }

    .newsbox2 .news {
        color: #fecf8f;
    }

    .funintro {
        display: flex;
        margin-top: 90rpx;
    }

    .introcell {
        font-size: 20rpx;
        color: #242627;
        display: flex;
        flex-direction: column;
        align-items: center;
        position: relative;
    }

    .introcell image {
        width: 60rpx;
        height: 60rpx;
        margin-bottom: 20rpx;
    }

    .introtext {
        font-size: 20rpx;
        color: #999;
        text-align: center;
        position: absolute;
        top: 120rpx;
        left: 50%;
        transform: translate(-50%, 0);
        line-height: 1.4;
    }

    .nextimg {
        width: 28rpx;
        height: 28rpx;
    }

    .signbox {
        height: 100rpx;
        display: flex;
        justify-content: space-between;
        align-items: center;
        padding: 0 30rpx;
        box-sizing: border-box;
        position: relative;
        background: rgba(255, 219, 9, .2);
        border-radius: 10rpx;
        margin-top: 50rpx;
        line-height: 1;
    }

    .signbox_left {
        display: flex;
        align-items: center;
    }

    .signbox_left image {
        width: 42rpx;
        height: 36rpx;
        margin-right: 14rpx;
    }

    .signbtn {
        height: 52rpx;
        padding: 0 20rpx;
        box-sizing: border-box;
        text-align: center;
        line-height: 52rpx;
        border-radius: 26rpx;
        font-size: 24rpx;
        background: #ffdb09;
        color: #242627;
        font-weight: 500;
    }

    .switchbtn {
        height: 60rpx;
        border-radius: 30rpx;
        border: 2rpx solid #FFDB09;
        line-height: 60rpx;
        text-align: center;
        font-size: 24rpx;
        display: flex;
        align-items: center;
        justify-content: space-between;
        box-sizing: border-box;
    }


    .center-vertical {
        display: flex;
        align-items: center;
        justify-content: space-between;
    }