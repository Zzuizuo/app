.invitebox .text {
  margin: 200rpx 0 0 30rpx;
  color: #999;
  font-size: 32rpx;
}
.invitebox .query_num_block {
  padding: 0 60rpx;
  box-sizing: border-box;
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
}
.invitebox .num_item_block {
  height: 80rpx;
  width: 70rpx;
  border-bottom: 4rpx solid #FFDB09;
  line-height: 80rpx;
  text-align: center;
  font-size: 48rpx;
  color: #242627;
  margin-top: 40rpx;
  text-transform: uppercase;
}
.invitebox .hidden_ipt {
  width: 690rpx;
  margin: 70rpx auto 70rpx auto;
  font-size: 48rpx;
  height: 80rpx;
  line-height: 80rpx;
  letter-spacing: 10rpx;
}
.invitebox .invitebtn {
  width: 690rpx;
  height: 80rpx;
  border-radius: 40rpx;
  background: #FFDB09;
  color: #242627;
  font-size: 28rpx;
  margin: 100rpx auto 0 auto;
}
.pagetop .boxitem {
  padding: 40rpx 30rpx;
  color: #242627;
  line-height: 1;
  border-bottom: 2rpx solid #f0f0f0;
}
.pagetop .boxitem:nth-child(1),
.pagetop .boxitem:nth-child(3) {
  border-bottom: 0;
}
.pagetop .boxitem:nth-last-child(1) {
  border-bottom: 0;
}
.pagetop .boxitem .boxtitle {
  font-size: 28rpx;
  margin-bottom: 20rpx;
  font-weight: 500;
}
.pagetop .boxitem .boxname {
  font-size: 32rpx;
  font-weight: bold;
}
.pagetop .boxitem .boxbtn {
  width: 160rpx;
  height: 60rpx;
  font-size: 24rpx;
  border-radius: 30rpx;
  background: #FFDB09;
}
.pagetop .boxitem .boxtext {
  font-weight: 500;
  font-size: 28rpx;
  margin-top: 20rpx;
}
.pagetop .boxitem .processline {
  width: 400rpx;
  height: 20rpx;
  border-radius: 10rpx;
  background: #FFEAE5;
  position: relative;
}
.pagetop .boxitem .processline .activeline {
  position: absolute;
  top: 0;
  left: 0;
  height: 20rpx;
  border-radius: 10rpx;
  background: #FF4925;
}
.pagetop .boxitem .taglist {
  margin-top: 20rpx;
}
.pagetop .boxitem .taglist .tagcell {
  height: 30rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
  font-size: 20rpx;
  background: #FFECE9;
  border: 1rpx solid #FF4925;
  margin-right: 10rpx;
}
.updatamask {
  position: relative;
  padding-top: 200rpx;
  line-height: 1;
}
.updatamask .updataimg {
  position: absolute;
  width: 590rpx;
  top: -222rpx;
  left: 0;
}
.updatamask .masktag {
  position: absolute;
  top: 120rpx;
  left: 50%;
  transform: translate(-50%, 0);
  font-size: 48rpx;
  color: #fff;
  font-weight: 500;
}
.updatamask .title {
  font-size: 32rpx;
  color: #242627;
  font-weight: 500;
  margin-top: 50rpx;
}
.updatamask .text1 {
  line-height: 1.4;
  font-size: 24rpx;
  color: #999;
  margin-top: 30rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.intromask {
  line-height: 1;
}
.intromask image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 100%;
  display: block;
}
.intromask .text1 {
  font-size: 32rpx;
  max-width: 500rpx;
  margin-top: 30rpx;
  font-weight: 500;
}
.intromask .text2 {
  font-size: 28rpx;
  margin-top: 80rpx;
  color: #999;
  position: relative;
}
.intromask .text2::after {
  content: '';
  position: absolute;
  top: 50%;
  left: -40rpx;
  transform: translate(0, -50%);
  width: 30rpx;
  border-top: 2rpx solid #999;
}
.intromask .text2::before {
  content: '';
  position: absolute;
  top: 50%;
  right: -40rpx;
  transform: translate(0, -50%);
  width: 30rpx;
  border-top: 2rpx solid #999;
}
.intromask .boxitem {
  width: 530rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
  position: relative;
  font-size: 28rpx;
  color: #242627;
  margin-top: 30rpx;
  font-weight: 500;
}
.intromask .boxitem::after {
  position: absolute;
  left: 0;
  top: 8rpx;
  content: '';
  width: 10rpx;
  height: 10rpx;
  border-radius: 100%;
  background: #FFDB09;
}
.intromask .maskbtn {
  width: 530rpx;
  height: 80rpx;
  background: #FFDB09;
  color: #242627;
  border-radius: 40rpx;
  font-size: 28rpx;
  margin-top: 70rpx;
}
.intromask .text3 {
  font-size: 20rpx;
  margin-top: 20rpx;
  color: #999;
}
.tipsmask {
  line-height: 1;
  color: #242627;
}
.tipsmask .maskcardtitle {
  font-size: 36rpx;
  font-weight: 600;
}
.tipsmask .masktext {
  color: #999;
  font-size: 32rpx;
  margin-top: 50rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  line-height: 1.4;
}
.sharerbox {
  width: 690rpx;
  border-radius: 10rpx;
  padding: 30rpx;
  box-sizing: border-box;
  line-height: 1;
  background: #FFF8CE;
  font-size: 24rpx;
  color: #242627;
  margin: 40rpx auto;
  position: relative;
}
.sharerbox::after {
  content: '';
  width: 0;
  height: 0;
  position: absolute;
  top: -18rpx;
  left: 50%;
  transform: translate(-50%, 0);
  border-right: 20rpx solid transparent;
  border-left: 20rpx solid transparent;
  border-bottom: 20rpx solid #FFF8CE;
}
.sharerbox .text1 {
  color: #999;
}
.sharerbox image {
  width: 60rpx;
  height: 60rpx;
  border-radius: 100%;
  display: block;
}
.sharerbox .boxbtn {
  width: 120rpx;
  height: 60rpx;
  border-radius: 30rpx;
  font-size: 24rpx;
  box-sizing: border-box;
}
.searchbox {
  padding: 0 30rpx;
  box-sizing: border-box;
  margin-top: 40rpx;
  position: relative;
}
.navbox {
  padding: 0 30rpx;
  box-sizing: border-box;
  padding-top: 30rpx;
  position: sticky;
