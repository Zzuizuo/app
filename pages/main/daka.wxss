/* pages/main/daka.wxss */

page {
    background: #f6f8fa;
}

.page {
    position: relative;
}

.pagetop {
    height: 340rpx;
    width: 100vw;
    box-sizing: border-box;
    position: relative;
    z-index: -2;
}

.pagetop image {
    position: absolute;
    top: 0;
    left: 0;
    width: 750rpx;
    height: 696rpx;
    z-index: -1;
    background: #FFDB09;
}

.processbox {
    padding: 0 30rpx;
    box-sizing: border-box;
}

.processContent {
    padding-top: 232rpx;
    padding-bottom: 50rpx;
    box-sizing: border-box;
    position: relative;
    border-radius: 20rpx;
    background: #fff;
}

.processTitle {
    width: 446rpx;
    height: 68rpx;
    line-height: 68rpx;
    text-align: center;
    font-size: 32rpx;
    color: #242627;
    position: absolute;
    top: -30rpx;
    left: 50%;
    transform: translate(-50%, 0);
    border-radius: 10rpx;
    background-size: cover;
    font-weight: bold;
}

.processTitlebg {
    width: 446rpx;
    height: 68rpx;
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
}

.proccessline {
    height: 10rpx;
    width: 560rpx;
    background: #f5f5f5;
    margin: 0 auto;
    display: flex;
    position: relative;
}

.activeLine {
    position: absolute;
    height: 10rpx;
    background: #FFDB09;
    left: 0;
    top: 0;
}

.activebox {
    width: 30rpx;
    height: 10rpx;
    position: relative;
}

.giftbox {
    width: 88rpx;
    height: 100rpx;
    text-align: center;
    position: absolute;
    top: -130rpx;
    left: -30rpx;
    font-size: 24rpx;
    text-align: center;
    z-index: 0;
}

.exchange {
    color: #FFB409;
    font-weight: bold;
    line-height: 88rpx;
}

.popnum {
    text-align: center;
    color: #999;
    margin-top: 20rpx;
}

.activebox image {
    position: absolute;
    top: 0;
    left: 0;
    width: 88rpx;
    height: 100rpx;
    z-index: -1;
}

.circle {
    width: 30rpx;
    height: 30rpx;
    border-radius: 100%;
    background: #f5f5f5;
    position: absolute;
    top: -10rpx;
    left: 0;
}

.popname {
    max-width: 150rpx;
    position: absolute;
    bottom: -54rpx;
    left: 50%;
    transform: translate(-50%, 0);
    font-size: 24rpx;
    text-align: center;
    line-height: 1;
    font-weight: bold;
}

.btnbox {
    margin: 120rpx auto 0 auto;
    width: 560rpx;
    height: 80rpx;
    position: relative;
}

.dakabtn {
    width: 560rpx;
    height: 80rpx;
    font-size: 32rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 40rpx;
    background: #FFDB09;
    color: #242627;
    font-weight: bold;
}

.rankbox {
    padding: 0 30rpx;
    box-sizing: border-box;
}

.rankcontent {
    padding: 50rpx 30rpx 30rpx;
    box-sizing: border-box;
    margin-top: 50rpx;
    background: #fff;
    border-radius: 20rpx;
}

.ranktitle {
    font-size: 32rpx;
    text-align: center;
    color: #242627;
    line-height: 1;
    font-weight: bold;
}

.ranklist {
    display: flex;
    padding: 50rpx 0 30rpx 0;
}

.rankcell {
    flex: 1;
    color: #242627;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.celltop {
    width: 126rpx;
    height: 126rpx;
    border-radius: 100%;
    position: relative;
}

.rankcell:nth-child(1) .celltop {
    border: 4rpx solid rgba(255, 219, 9, .8);
}

.rankcell:nth-child(2) .celltop {
    border: 4rpx solid rgba(255, 73, 37, .8);
}

.rankcell:nth-child(3) .celltop {
    border: 4rpx solid rgba(55, 169, 254, .8);
}

.rankheadimg {
    width: 126rpx;
    height: 126rpx;
    border-radius: 100%;
}

.rankicon {
    position: absolute;
    width: 148rpx;
    height: 52rpx;
    top: 88rpx;
    left: 50%;
    transform: translate(-50%);
}

.cellmiddle {
    font-size: 28rpx;
    line-height: 1;
    margin-top: 30rpx;
}

.cellbottom {
    font-size: 24rpx;
    margin-top: 20rpx;
}

.myrank {
    text-align: center;
    font-size: 24rpx;
    color: #242627;
    padding-top: 30rpx;
    border-top: 1rpx solid #f5f5f5;
}

.introbox {
    margin-top: 100rpx;
    padding-bottom: 190rpx;
}

.introtitle,
.ruletitle {
    font-size: 32rpx;
    color: #242627;
    text-align: center;
    line-height: 1;
    font-weight: bold;
}

.stepbox {
    width: 550rpx;
    margin: 50rpx auto 0 auto;
    display: flex;
}

.stepcell {
    width: 60rpx;
    height: 60rpx;
    border-radius: 100%;
    position: relative;
    background: rgba(255, 219, 9, .5);
    z-index: 1;
    margin-left: 185rpx;
}

.stepcell:nth-child(1) {
    margin-left: 0;
}

.stepcell::after {
    content: '';
    position: absolute;
    width: 165rpx;
    left: 70rpx;
    top: 30rpx;
    border-bottom: 2rpx dashed rgba(255, 219, 9, .8);
}

.stepcell:nth-child(3)::after {
    border-bottom: none;
}

.circle_w {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    content: '';
    border-radius: 100%;
    width: 48rpx;
    height: 48rpx;
    border-radius: 100%;
    position: relative;
    background: #fff;
    z-index: 2;
}

.circle_y {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    width: 40rpx;
    height: 40rpx;
    border-radius: 100%;
    background: #FFDB09;
    color: #242627;
    line-height: 40rpx;
    text-align: center;
    font-size: 28rpx;
    z-index: 3;
}

.steptext {
    position: absolute;
    top: 100rpx;
    left: 50%;
    transform: translate(-50%, 0);
    width: 150rpx;
    font-size: 24rpx;
    color: #242627;
    text-align: center;
    line-height: 1.2;
}

.rulebox {
    padding: 0 60rpx 100rpx;
}

.qrcodebox {
    width: 100vw;
    height: 400rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background: #fff;
    font-size: 24rpx;
}

.qrcodebox image {
    width: 200rpx;
    height: 200rpx;
}

.coverimg {
    position: absolute;
    top: 90rpx;
    left: 50%;
    transform: translate(-50%, 0);
    width: 450rpx;
    height: 800rpx;
    z-index: 1200;
}

.savebox {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100vw;
    height: 480rpx;
    /* padding-top: 214rpx; */
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background: #fff;
    font-size: 28rpx;
    z-index: 1111;
}

.savebox image {
    width: 60rpx;
    height: 60rpx;
}

.savetext {
    font-size: 28rpx;
    line-height: 1;
}

.saveimgbtn {
    position: absolute;
    bottom: 50rpx;
    left: 50%;
    transform: translate(-50%, 0);
    width: 600rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 32rpx;
    text-align: center;
    background: #FFDB09;
    color: #242627;
    border-radius: 40rpx;
    /* margin: 40rpx auto 0 auto; */
    font-weight: bold;
}

.sharetext {
    display: flex;
    align-items: center;
    justify-content: center;
    line-height: 1;
    font-size: 28rpx;
}

.continue {
    color: #FF4925;
}

.gifttext {
    height: 40rpx;
    position: absolute;
    right: 0;
    top: -48rpx;
    font-size: 24rpx;
    line-height: 40rpx;
    background: #FF4925;
    color: #fff;
    padding: 0 20rpx;
    border-top-right-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
    border-top-left-radius: 10rpx;
}

.gifttext::after {
    content: '';
    width: 0;
    height: 0;
    border-right: 10rpx solid transparent;
    border-top: 10rpx solid #FF4925;
    position: absolute;
    bottom: -10rpx;
    left: 0;
}

.nogift {
    position: absolute;
    left: 50%;
    top: 120rpx;
    transform: translate(-50%, 0);
    display: flex;
    align-items: center;
    justify-content: center;
}


.listcell {
    width: 520rpx;
    height: 152rpx;
    border-radius: 4rpx;
    display: flex;
    position: relative;
    background: #FFF;
    box-shadow: 0 0 10rpx #e6e6e6;
}

.wheelbox{
    width: 520rpx;
    height: 152rpx;
    border-radius: 4rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #FFF;
    box-shadow: 0 0 10rpx #e6e6e6;
}

.listcell::before {
    content: '';
    position: absolute;
    left: -15rpx;
    background: #fff;
    top: 50%;
    transform: translate(0, -50%);
    width: 30rpx;
    height: 30rpx;
    border-radius: 100%;
    box-shadow: 0 0 10rpx #e6e6e6 inset;
    z-index: 8;
}

.listcell::after {
    content: '';
    position: absolute;
    right: -15rpx;
    top: 50%;
    transform: translate(0, -50%);
    width: 30rpx;
    height: 30rpx;
    border-radius: 100%;
    background: #fff;
    box-shadow: 0 0 10rpx #e6e6e6 inset;
}

.circlecover {
    position: absolute;
    right: -15rpx;
    top: 50%;
    transform: translate(0, -50%);
    width: 15rpx;
    height: 30rpx;
    background: #fff;
    z-index: 10;
}

.circlecover1 {
    position: absolute;
    left: -15rpx;
    top: 50%;
    transform: translate(0, -50%);
    width: 15rpx;
    height: 30rpx;
    background: #fff;
    z-index: 10;
}

.cellleft {
    width: 152rpx;
    height: 152rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    position: relative;
    background: #FF764C;
    color: #fff;
    border-right: 2rpx dashed #e6e6e6;
    font-size: 20rpx;

}

.cellleft image {
    width: 6rpx;
    height: 194rpx;
    position: absolute;
    top: 3rpx;
    right: -3rpx;
}

.sale {
    line-height: 1;
    text-align: center;
}

.cellright {
    width: 368rpx;
    height: 152rpx;
    padding-left: 30rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    justify-content: center;
    background: #FFFBFA;
}

.cellrightname {
    max-width: 280rpx;
    font-size: 28rpx;
    line-height: 1.2;
    color: #242627;
    font-weight: bold;
    text-align: left;
}

.shop {
    width: 300rpx;
    font-size: 24rpx;
    color: #666;
    margin-top: 20rpx;
    line-height: 1;
    display: flex;
    
}
.seleteimg{
    width: 60rpx;height: 60rpx;border-top-right-radius: 10rpx;
    position: absolute;
    top: 0;
    right: 0;
}

@media screen and (min-height: 680px) {
    .sharetext {
        margin-top: 50rpx;
    }
    .savetext {
        margin-top: 50rpx;
    }
    .saveimgbtn {
        bottom: 100rpx;
    }
}