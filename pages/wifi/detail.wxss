page {
    background-color: #333333;
    font-family: -apple-system, "PingFang SC", "Helvetica Neue", "Arial", " Segoe UI", "Hiragino Sans GB", "STHeiti", "Microsoft YaHei", "Microsoft JhengHei", "Source Han Sans SC", "Noto Sans CJK SC", "Source Han Sans CN", "Noto Sans SC", "Source Han Sans TC", "Noto Sans CJK TC", "WenQuanYi Micro Hei", SimSun, sans-serif;
    font-size: 28rpx;
    line-height: 1.6;
    color: white;
}
.head{
    background-color: #232323;
}

.setting {
    padding: 30rpx 40rpx;
    border-bottom: 1rpx solid #222;
    display: flex;
    justify-content: space-between;
    align-items: center;
    /* line-height: 1.1; */
}

.actionbtn {
    padding: 0 20rpx;
    background-color: #36ca58;
    color: #fff;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
}

.connects {
    background-color: #111;
}

.connects-title {
    text-align: center;
    padding: 20rpx;
    color: #888;
}

.connect {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 30rpx;
}

.connect .user {
    display: flex;
    align-items: center;
}

.headtabs {
    display: flex;
    align-items: center;
    padding: 0 40rpx;
    height: 60rpx;
    background-color: #232323;
}

.headtab {
    width: 120rpx;
    padding-bottom: 20rpx;
}

.headtabs-cusor {
    background-color: #36ca58;
    border-radius: 8rpx;
    width: 56rpx;
    height: 8rpx;
    position: absolute;
    margin-top: -4rpx;
}

.body {
    background-color: #333333;
    min-height: calc(100vh - 200rpx);
}


.group {
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: #f1f1f1;
    margin-top: 10rpx;
    border-radius: 8rpx;
    padding: 0 20rpx;
}

.circle {
    background-color: #36ca58;
    border-radius: 40rpx;
    height: 40rpx;
    width: 40rpx;
    border: 2rpx solid #333;
    margin-right: 10rpx;
}